{
  "version": 2,
  "builds": [
    {
      "src": "api/simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/simple.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/simple.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/simple.py"
    }
  ]
}
